/* itemTemplate */
.itemTemplate {
  width: 95vw;
  background-color: #fff;
  margin: 20rpx auto 0;
  border-radius: 20rpx;
}

.item{
  font-size: 70rpx;
  font-weight: bold;
  margin: 20rpx auto 0;
}


.carBottom {
  display: flex;
  flex-direction: row;
}
 
 
.item-left {
   flex: 1; 
   align-items:center;
   font-size: 60rpx;
   font-weight: bold;
}
 
.item-right {
  position: relative;
  align-items:center;
  font-size: 60rpx;
  font-weight: bold;
}
